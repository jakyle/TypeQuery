{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-eslint-rules"
    ],
    "jsRules": {},
    "rules": {
        "unified-signatures": false,
        "object-literal-sort-keys": false,
        "quotemark": [true, "single"],
        "no-console": {
            "severity": "none",
            "options": [
                "debug",
                "info",
                "log",
                "time",
                "timeEnd",
                "trace"
            ]
        },
        "variable-name": [
            true,
            "allow-leading-underscore"
        ]
    },
    "rulesDirectory": []
}
